def isVowel(ch):
    if(len(ch)>1):
        print("I said one letter")
    elif(ch=='A' or ch=='a' or ch=='E' or ch =='e' or ch=='I' or ch=='i' or ch=='O' or ch=='o' or ch=='U' or ch=='u'):
        print(ch, "is a Vowel")
    else:
        print(ch, "is a Consonant")

def main():
    ch = input("Enter a character: ")
    isVowel(ch)
    
if __name__ == "__main__":
    main()
